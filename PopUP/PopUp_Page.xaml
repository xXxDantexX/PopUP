<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PopUP.PopUp_Page">
    <ContentPage.Content>
        <StackLayout Padding="10">
            <Label Text="Korrutustabel" FontSize="Large" HorizontalOptions="Center" Margin="0,10,0,20" TextColor="Black" />
            <StackLayout>
                <Label Text="Sisesta oma nimi:" FontSize="Medium" TextColor="Black" />
                <Entry x:Name="NameEntry" Placeholder="Nimi" />
            </StackLayout>
            <Button Text="Alasta testi" Clicked="OnStartTestClicked" HorizontalOptions="Center" Margin="0,20,0,0" />
            <Button Text="Tagasi" Clicked="Button_Clicked" HorizontalOptions="Center" Margin="0,20,0,0" />
            <Button Text="Faili kustutamine" Clicked="OnDeleteButtonClicked" HorizontalOptions="Center" Margin="0,20,0,0" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>